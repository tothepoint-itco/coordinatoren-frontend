{
    "compilerOptions": {
        "outFile": "app/compiled.js",
        "removeComments": true,
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "app/**/*.ts",
        "app/**/*.tsx",
        "!app/bower_components/**"
    ],
    "atom": {
        "rewriteTsconfig": true
    },
    "files": [
        "app/app.ts",
        "app/controllers/akkoord-controller.ts",
        "app/controllers/bediende-controller.ts",
        "app/controllers/bediende-details-controller.ts",
        "app/controllers/beheerder-controller.ts",
        "app/controllers/bestelbon-controller.ts",
        "app/controllers/business-unit-controller.ts",
        "app/controllers/contract-controller.ts",
        "app/controllers/directives/timeline-controller.ts",
        "app/controllers/directives/timeline-overview-controller.ts",
        "app/controllers/login-controller.ts",
        "app/controllers/main-controller.ts",
        "app/controllers/modals/akkoord-modal-controller.ts",
        "app/controllers/modals/bestelbon-modal-controller.ts",
        "app/controllers/modals/confirm-modal-controller.ts",
        "app/controllers/modals/create-akkoord-controller.ts",
        "app/controllers/modals/create-bediende-controller.ts",
        "app/controllers/modals/create-beheerder-controller.ts",
        "app/controllers/modals/create-bestelbon-controller.ts",
        "app/controllers/modals/create-business-unit-controller.ts",
        "app/controllers/modals/create-contract-controller.ts",
        "app/controllers/modals/create-opdracht-controller.ts",
        "app/controllers/modals/create-rol-controller.ts",
        "app/controllers/modals/create-user-controller.ts",
        "app/controllers/opdracht-controller.ts",
        "app/controllers/planning-controller.ts",
        "app/controllers/rol-controller.ts",
        "app/controllers/user-controller.ts",
        "app/models/akkoord.ts",
        "app/models/bediende.ts",
        "app/models/beheerder.ts",
        "app/models/bestelbon.ts",
        "app/models/business-unit.ts",
        "app/models/consultant-aggregated.ts",
        "app/models/consultant.ts",
        "app/models/contract-aggregated.ts",
        "app/models/contract.ts",
        "app/models/login.ts",
        "app/models/opdracht.ts",
        "app/models/rol.ts",
        "app/models/user.ts"
    ]
}
